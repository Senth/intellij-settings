<keymap version="1" name="Senth keymap" parent="$default">
  <action id="CheckinProject">
    <keyboard-shortcut first-keystroke="shift ctrl g" second-keystroke="shift ctrl c" />
  </action>
  <action id="EditorDeleteLine">
    <keyboard-shortcut first-keystroke="ctrl enter" />
  </action>
  <action id="Git.Pull">
    <keyboard-shortcut first-keystroke="shift ctrl g" second-keystroke="shift ctrl g" />
  </action>
  <action id="Vcs.Push">
    <keyboard-shortcut first-keystroke="shift ctrl g" second-keystroke="shift ctrl s" />
  </action>
</keymap>